// Place your key bindings in this file to overwrite the defaults
[
  {
      "key": "ctrl+alt+enter",
      "command": "colonize.endline",
      "when": "editorTextFocus"
  },
  {
      "key": "shift+enter",
      "command": "-colonize.endline",
      "when": "editorTextFocus"
  },
  {
      "key": "shift+enter",
      "command": "colonize.hold",
      "when": "editorTextFocus"
  },
  {
      "key": "ctrl+alt+enter",
      "command": "-colonize.hold",
      "when": "editorTextFocus"
  },
  {
      "key": "alt+up",
      "command": "-editor.action.moveLinesUpAction",
      "when": "editorTextFocus && !editorReadonly"
  },
  {
      "key": "alt+down",
      "command": "-editor.action.moveLinesDownAction",
      "when": "editorTextFocus && !editorReadonly"
  },
  {
      "key": "cmd+k",
      "command": "cursorUp",
      "when": "textInputFocus"
  },
  {
      "key": "cmd+j",
      "command": "cursorLeft",
      "when": "textInputFocus"
  },
  {
      "key": "ctrl+b",
      "command": "-cursorLeft",
      "when": "textInputFocus"
  },
  {
      "key": "cmd+;",
      "command": "cursorRight",
      "when": "textInputFocus"
  },
  {
      "key": "ctrl+f",
      "command": "-cursorRight",
      "when": "textInputFocus"
  },
  {
      "key": "cmd+l",
      "command": "cursorDown",
      "when": "textInputFocus"
  },
  {
      "key": "ctrl+n",
      "command": "-cursorDown",
      "when": "textInputFocus"
  },
  {
      "key": "alt+;",
      "command": "cursorWordPartRight",
      "when": "textInputFocus"
  },
  {
      "key": "alt+right",
      "command": "-cursorWordPartRight",
      "when": "textInputFocus"
  },
  {
      "key": "shift+alt+;",
      "command": "cursorWordPartRightSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+alt+right",
      "command": "-cursorWordPartRightSelect",
      "when": "textInputFocus"
  },
  {
      "key": "alt+j",
      "command": "cursorWordPartLeft",
      "when": "textInputFocus"
  },
  {
      "key": "alt+left",
      "command": "-cursorWordPartLeft",
      "when": "textInputFocus"
  },
  {
      "key": "shift+alt+j",
      "command": "cursorWordPartLeftSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+alt+left",
      "command": "-cursorWordPartLeftSelect",
      "when": "textInputFocus"
  },
  {
      "key": "cmd+i",
      "command": "workbench.action.focusLastEditorGroup",
      "when": "!textInputFocus",
  },
  {
      "key": "alt+k",
      "command": "cursorMove",
      "when": "textInputFocus",
      "args": {
          "to": "up",
          "by": "line",
          "value": 10
      },
  },
  {
      "key": "pageup",
      "command": "-cursorPageUp",
      "when": "textInputFocus"
  },
  {
      "key": "alt+l",
      "command": "cursorMove",
      "when": "textInputFocus",
      "args": {
          "to": "down",
          "by": "line",
          "value": 10
      },
  },
  {
      "key": "pagedown",
      "command": "-cursorPageDown",
      "when": "textInputFocus"
  },
  {
      "key": "cmd+'",
      "command": "cursorEnd",
      "when": "textInputFocus"
  },
  {
      "key": "end",
      "command": "-cursorEnd",
      "when": "textInputFocus"
  },
  {
      "key": "shift+cmd+l",
      "command": "cursorDownSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+down",
      "command": "-cursorDownSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+cmd+k",
      "command": "cursorUpSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+up",
      "command": "-cursorUpSelect",
      "when": "textInputFocus"
  },
  {
      "key": "cmd+h",
      "command": "cursorHome",
      "when": "textInputFocus"
  },
  {
      "key": "home",
      "command": "-cursorHome",
      "when": "textInputFocus"
  },
  {
      "key": "shift+cmd+h",
      "command": "cursorHomeSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+home",
      "command": "-cursorHomeSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+cmd+'",
      "command": "cursorEndSelect",
      "when": "textInputFocus"
  },
  {
      "key": "shift+end",
      "command": "-cursorEndSelect",
      "when": "textInputFocus"
  },
  {
      "key": "alt+backspace",
      "command": "deleteWordPartLeft",
      "when": "textInputFocus && !editorReadonly"
  },
  {
      "key": "shift+ctrl+backspace",
      "command": "-deleteWordLeft",
      "when": "textInputFocus && !editorReadonly"
  },
  {
      "key": "cmd+i",
      "command": "github.copilot.terminal.suggestCommand",
      "when": "terminalFocus"
  },
  {
      "key": "cmd+i",
      "command": "-github.copilot.terminal.suggestCommand",
      "when": "terminalFocus"
  },
  {
      "key": "cmd+i",
      "command": "workbench.action.files.openLocalFileFolder",
      "when": "remoteFileDialogVisible"
  },
  {
      "key": "cmd+o",
      "command": "-workbench.action.files.openLocalFileFolder",
      "when": "remoteFileDialogVisible"
  }
]
